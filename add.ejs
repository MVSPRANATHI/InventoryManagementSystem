<html lang="en">
    <head>
        <title>Accessories</title>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="plugins/fontawesome-free/css/all.min.css">
  <!-- Theme style -->
  <link rel="stylesheet" href="dist/css/adminlte.min.css">
  <!-- Google Font: Source Sans Pro -->
  <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
        
    </head>
    <body class="hold-transition sidebar-mini">
	
        <div class="wrapper">
            
            <!-- Navbar -->
            <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <!-- Left navbar links -->
                <ul class="navbar-nav">
                    
                    <li class="nav-item d-none d-sm-inline-block">
                    <a href="/" class="nav-link">Home</a>
                    </li>
                </ul>
            </nav>
            <!-- /.navbar -->
            
            <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
                <a href="/" class="brand-link">
                       <span class="brand-text font-weight-light">Accessories</span>
                </a>
                <!-- Sidebar -->
                <div class="sidebar">
                    <nav class="mt-2">
                        <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                          <li class="nav-item">
                            <a href="/adddata" class="nav-link">
                              <i class="nav-icon far fa-circle text-info"></i>
                              <p>Add Product</p>
                            </a>
                          </li>
                          <li class="nav-item">
                            <a href="/deletestock" class="nav-link">
                              <i class="nav-icon far fa-circle text-info"></i>
                              <p class="text">Delete product</p>
                            </a>
                          </li>
                          <li class="nav-item">
                            <a href="/stockupdate" class="nav-link">
                              <i class="nav-icon far fa-circle text-info"></i>
                              <p class="text">Update Stock</p>
                            </a>
                          </li>
                          
                        </ul>
                    </nav>
                </div>
                <!-- /.sidebar -->
            </aside>
            
            <!-- Content Wrapper. Contains page content -->
            <div class="content-wrapper">
                
                <section class="content-header">
                    <h1>Add a product</h1>
                </section>
                
                <section class="content">
                    <div class="card">
                    
                          
                        <div class="card-header">
                           
                            <div class="card-body table-responsive p-0" style="height: 400px;">
                                <form action="/AddData" method="POST">
                                    <label>Watch Id:</label>
                                    <input type="String" placeholder="Enter Watch Id" name="WatchId"><br>
                        
                                    <label>Watch Brand:</label>&nbsp;
                                    <input type="String" placeholder="Enter Watch Brand" name="WatchBrand"><br>
                        
                                    <label>Watch Style:</label>&nbsp;
                                    <input type="String" placeholder="Watch Style" name="WatchStyle"><br>
                        
                                    <label>Watch Display:</label>
                                    <input type="String" placeholder="Enter Watch Display" name="WatchDisplay"><br>
                        
                                    <label>Stock:</label>&nbsp;
                                    <input type="String" placeholder="Enter quantity" name="Stock"><br>
                        
                                    <label>Cost Price:</label>&nbsp;
                                    <input type="String" placeholder="Enter cost price" name="CostPrice"><br>
                        
                                    <label>Selling price:</label>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                    <input type="String" placeholder="Enter selling price " name="SellingPrice"><br><br>
                        
                                    <button type="submit">Submit</button>
                        
                                </form>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
            
        </div>
    </body>
</html>

